								"exec": [
									"var body = JSON.parse(responseBody);",
									"var meta_data = body[\"data\"][\"metadata\"];",
									"",
									"",
									"let envariable = \"\";",
									"for (var i = 0; i < meta_data.length; i++) {",
									"    let temp_guid = meta_data[i][\"guid\"];",
									"",
									"    envariable = 'dv_guid_' + i;",
									"    postman.setEnvironmentVariable(envariable, temp_guid)",
									"}"
								],
