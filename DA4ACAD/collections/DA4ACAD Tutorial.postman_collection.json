							"script": {
								"id": "7c7a5754-f73b-438a-b2f9-6951fe05a451",
								"exec": [
									"var body = JSON.parse(responseBody);",
									"",
									"pm.environment.set(\"ossOutputFileSignedUrl\", body[\"signedUrl\"]);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
