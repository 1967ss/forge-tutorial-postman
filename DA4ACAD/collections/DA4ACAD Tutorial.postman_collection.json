					},
					"response": []
				},
				{
					"name": "Get Temporary Upload URL for the Output Text File",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"id": "fbfce144-349e-4ea7-b647-79f6cb972ed1",
								"exec": [
									""
