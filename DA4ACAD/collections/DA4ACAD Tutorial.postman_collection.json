							{
								"key": "Authorization",
								"value": "Bearer {{dasApiToken}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{}\n"
						},
						"url": {
							"raw": "https://developer.api.autodesk.com/oss/v2/buckets/{{ossBucketKey}}/objects/{{ossDwgFileObjectKey}}/signed",
							"protocol": "https",
							"host": [
								"developer",
								"api",
								"autodesk",
								"com"
							],
							"path": [
								"oss",
								"v2",
								"buckets",
								"{{ossBucketKey}}",
								"objects",
								"{{ossDwgFileObjectKey}}",
								"signed"
